package org.generation.util;

public class PayRoll {
		
	public static void printReceipt(Employee e) {
		System.out.println("============================");
		System.out.println("| Recibo de pago de: " );
		System.out.println("| Nombre: " + e.getName());
		System.out.println("| Nombre: " + e.getRFC());
		System.out.println("| Por la cantidad de: " + e.calculateSalary(30));
		System.out.println("============================");
	}//printReceipt
	
}//class PayRoll
