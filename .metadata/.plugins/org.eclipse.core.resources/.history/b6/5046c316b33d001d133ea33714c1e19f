
public class Person {
	String name;
	int age;
	String email;
	String rfc;
	String address;
	
public Person(String name, int age, String email, String address, String rfc) {
		this.name = name;
		this.age = age;
		this.email = email;
		this.address = address;
		this.rfc = rfc;
	}//constructor

public Person(String name, int age, String email, String address) {
	this.name = name;
	this.age = age;
	this.email = email;
	this.address = address;
}//constructor
	
	
}// class Person
